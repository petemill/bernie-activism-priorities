{
  "name": "bernie-activism-priorities",
  "description": "A web-app for determining what the priorities are right now in campaigning for Bernie Sanders for the 2016 USA Presidential Election",
  "version": "0.1.0",
  "author": "Pete Miller <miller.pete@gmail.com>",
  "license": "MIT",
  "keywords": [
    "bernie",
    "sanders",
    "activism",
    "map",
    "react",
    "redux",
    "es6",
    "hmr",
    "hot",
    "babel",
    "webpack",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/petemill/bernie-activism-priorities.git"
  },
  "bugs": "https://github.com/petemill/bernie-activism-priorities/issues",
  "scripts": {
    "clean:build": "rm -rf build && mkdir build",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "rm -rf build",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "SET NODE_ENV=production& NODE_ENV=production& webpack -p --config ./config/webpack.config.prod.js --progress",
    "lint": "eslint config src/js/**/*.js",
    "jscs": "jscs src/js/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "finesse": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js -p --config ./config/webpack.config.prod.js --progress"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.5.2",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.3",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "berniestrap": "SandersForPresident/bootstrap#master",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "0.3.3",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "imports-loader": "0.6.5",
    "jscs": "2.10.1",
    "json-loader": "0.5.4",
    "node-sass": "3.4.2",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.1.1",
    "redux-devtools-dock-monitor": "1.1.0",
    "redux-devtools-log-monitor": "1.0.4",
    "resolve-url-loader": "1.4.3",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  },
